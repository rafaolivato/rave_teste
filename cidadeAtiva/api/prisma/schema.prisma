generator client {
  provider = "prisma-client-js"
  
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Perfil {
  id           Int      @id @default(autoincrement())
  nome         String
  sobrenome    String
  cpf          String   @unique
  tipoTelefone String
  telefone     String
  email        String   @unique
  criadoEm     DateTime @default(now())
}
